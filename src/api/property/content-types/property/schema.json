{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": true
    },
    "BlockchainPropertyId": {
      "type": "string",
      "unique": true
    },
    "Images": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "FormattedAddress": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "Address1": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Address2": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Address3": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Address4": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Address5": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Description": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Location": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "PricePerNight": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "unique": false
    },
    "Rooms": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": 1
    },
    "Bathrooms": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": 1
    },
    "Size": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "PurchasePrice": {
      "type": "decimal",
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "property_amenities": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::property-amenity.property-amenity",
      "inversedBy": "properties"
    },
    "locations": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "properties"
    },
    "Latitude": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Longitude": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "map": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Featured": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "property_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::property-review.property-review",
      "mappedBy": "property"
    },
    "currency": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::currency.currency",
      "inversedBy": "properties"
    },
    "Stars": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": 1,
      "min": 0,
      "max": 5
    },
    "CurrentlyRented": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false,
      "default": false
    },
    "property_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::property-type.property-type",
      "inversedBy": "properties"
    },
    "MaxGuests": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 1
    },
    "CleaningFee": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "AtlasFees": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": 0.5
    },
    "proeprty_booking": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::proeprty-booking.proeprty-booking",
      "mappedBy": "property"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "properties"
    },
    "PhoneNumber": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
